package Layer3;

public class IPframe {

	private char[] ver=new char[4];
	private char[] headerLength=new char[4];
	private char[] typeOfService=new char[8];
	private char[] identification=new char[16];
	private char[] timeToLive=new char[8];
	private char[] protocol=new char[8];
	private char[] totalLength=new char[16];
	private char[] flags=new char[3];
	private char[] offset=new char[13];
	private char[] checkSum=new char[16];
	private char[] sourceAddress=new char[32];
	private char[] destinationAddress=new char[32];
	private String option;
	private String data;


}
